generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["interactiveTransactions"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Password {
  id       Int    @id
  password String
}

model User {
  id          Int    @id @default(autoincrement())
  email       String @unique
  firstName   String
  lastName    String
  displayName String
  image       String
  role        String @default("user")
}
